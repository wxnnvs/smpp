{
	"manifest_version": 3,
	"name": "Smartschool 2",
	"version": "2.0.0",
	"description": "Enhance your smartschool experience with themes and more",
	"homepage_url": "https://github.com/wxnnvs/smpp",
	"content_scripts": [
		{
			"matches": [
				"*://*.smartschool.be/*"
			],
			"all_frames": true,
			"js": [
				"json.js",
				"sm2.js"
			]
		}
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "sm2@wxnnvs"
		}
	}
}
